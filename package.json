{
  "name": "solid-journal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@ldo/ldo": "^0.0.1-alpha.26",
    "@ldo/solid": "^0.0.1-alpha.26",
    "@ldo/solid-react": "^0.0.1-alpha.26",
    "@solid/community-server": "^7.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@tiptap/extension-code-block-lowlight": "^2.5.4",
    "@tiptap/extension-placeholder": "^2.5.4",
    "@tiptap/extension-table": "^2.5.4",
    "@tiptap/extension-table-cell": "^2.5.4",
    "@tiptap/extension-table-header": "^2.5.4",
    "@tiptap/extension-table-row": "^2.5.4",
    "@tiptap/extension-text-align": "^2.4.0",
    "@tiptap/extension-underline": "^2.4.0",
    "@tiptap/pm": "^2.4.0",
    "@tiptap/react": "^2.4.0",
    "@tiptap/starter-kit": "^2.4.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.101",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@uidotdev/usehooks": "^2.4.1",
    "antd": "^5.18.3",
    "lowlight": "^3.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^10.0.5",
    "typescript": "^4.9.5",
    "use-async-effect": "^2.2.7",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "concurrently \"npm run start\" \"npm run css\"",
    "start": "PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "css": "./node_modules/@solid/community-server/bin/server.js -c ./css/config.json -f ./data",
    "eject": "react-scripts eject",
    "build:ldo": "ldo build --input src/.shapes --output src/.ldo"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@ldo/cli": "^0.0.1-alpha.26",
    "@types/jsonld": "^1.5.14",
    "@types/shexj": "^2.1.7",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.3.2"
  },
  "eslintConfig": {
    "extends": [
      "prettier",
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ]
    }
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "public"
      ],
      "extensions": "*",
      "quiet": false
    }
  }
}
